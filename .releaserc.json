{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/github", {
      "assets": [
        {
          "path": "target/release/latest-sender",
          "name": "latest-sender-linux-x86_64",
          "label": "Linux x86_64 binary"
        },
        {
          "path": "target/x86_64-pc-windows-gnu/release/latest-sender.exe",
          "name": "latest-sender-windows-x86_64.exe",
          "label": "Windows x86_64 binary"
        },
        {
          "path": "target/x86_64-apple-darwin/release/latest-sender",
          "name": "latest-sender-macos-x86_64",
          "label": "macOS x86_64 binary"
        }
      ]
    }]
  ]
}